<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="counterMapper">
    
    <insert id="insertCounter" parameterType="Map">
        INSERT INTO COUNTER
        (MBR_ID, REG_DT, MBR_IP)
        VALUES
        (#{MBR_ID}, SYSDATE, #{MBR_IP})
    </insert>
</mapper>